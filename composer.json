{
  "name": "bentools/etl",
  "description": "Extract / Transform / Load in PHP - Multiple formats - No dependency.",
  "license": "MIT",
  "authors": [
    {
      "name": "Beno!t POLASZEK"
    }
  ],
  "require": {
    "php": ">=7.1"
  },
  "autoload-dev": {
    "psr-4": {
      "BenTools\\ETL\\Tests\\": "tests/src"
    },
    "files": [
      "vendor/symfony/var-dumper/Resources/functions/dump.php"
    ]
  },
  "autoload": {
    "psr-4": {
      "BenTools\\ETL\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "squizlabs/php_codesniffer": "~2.0",
    "symfony/event-dispatcher": "^3.2",
    "doctrine/orm": "^2.5",
    "guzzlehttp/promises": "^1.3",
    "satooshi/php-coveralls": "^1.0",
    "symfony/var-dumper": "^3.2"
  },
  "extra": {
    "branch-alias": {
      "dev-3.0": "3.0.x-dev"
    }
  }
}
